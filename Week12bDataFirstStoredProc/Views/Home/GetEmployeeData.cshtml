@* @model List<Week12bDataFirstStoredProc.Models.Employee> *@


@{
    ViewData["Title"] = "GetEmployeeData";
}

<h1>GetEmployeeData</h1>


<table class="table table-dark table-striped">
    <thead>
        <tr class="table-primary">
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>Delete</th>
            <th>Update</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Address</td>
                <td><a asp-controller="Home" asp-action="Delete" asp-route-id="@item.id">Delete</a></td>
                <td><a asp-controller="Home" asp-action="Update" asp-route-id="@item.id">Update</a></td>


            </tr>
        }

    </tbody>

</table>

